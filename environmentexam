pipeline {
  agent any
parameters {
  string defaultValue: 'build', description: 'building application', name: 'Env'
string defaultValue: 'branch', description: 'building application', name: 'branch'
  
}
  stages {
    stage('Parallel builds') {
      parallel {
        stage('build1') { steps { echo "building build1  $Env "} }
       stage('branch') { steps { echo "building build1  $branch"} }
      }
    }
  }
}
