pipeline {
  agent any
parameters {
  string defaultValue: 'build', description: 'building application', name: 'ENV'
string defaultValue: 'test', description: 'building application', name: 'branch1'
  
}


  stages {
    stage('Parallel builds') {
      parallel {
        stage('build1') { steps { echo "building building  $ENV"} }
       stage('branch2') { steps { echo "building of branch in $branch1"} }
      }
    }
  }
}
