pipeline {
  agent any
parameters {
  string defaultValue: 'build', description: 'building application', name: 'Env'
  choice choices: ['test'], description: 'testing envirn', name: 'branch'
}
  stages {
    stage('Parallel builds') {
      parallel {
        stage('build1') { steps { echo "building build1  $(params.env) "} }
       stage('test') { steps { echo "building build1  $(params.branch)"} }
      }
    }
  }
}
