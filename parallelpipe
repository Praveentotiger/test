pipeline {
  agent any
  stages {
    stage('Parallel builds') {
      parallel {
        stage('build1') { steps { echo 'building build1' 
                                        sh sleep 5  } }
        stage('build2') { steps { echo 'building build2'} }
        stage('build3') { steps { echo 'building build3'} }
      }
    }
  }
}
