pipeline {
  agent {
  label 'jenkins'
}

  stages {
    stage('build') {
      steps {
        echo 'hello'
        sh 'sleep 5'
      }
    }
    stage('deploy') {
      steps {
        echo 'hello'
        sh 'sleep 5'
      }
    }
    stage('test') {
      steps {
        echo 'hello'
        sh 'sleep 5'
      }
    }
  }
}
