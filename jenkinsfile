pipeline{ 
  agent any ;
  stages{  
    stage('build'){
            steps{ 
                  echo "hello"
                  sh 'sleep  5'
                  }
                  }
  stage('deploy'){
              steps{
                  echo "deploy in"
                  sh 'sleep 5'
}
                  }
stage('test'){
              steps{
              echo 'testing'
              sh 'sleep 6'
                  }
                  }
}
}
